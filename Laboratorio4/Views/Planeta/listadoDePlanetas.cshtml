
@{
    ViewBag.Title = "Planetas";
}
<!DOCTYPE html>
<html>
<head>
    <title> Lista de planetas </title>
    <meta charset="utf-8" />

</head>
<body>
    <h1> Resultado </h1>

    <div class="seccion_1" id="s1">
        <table class="table">

            <tr>

                <th> Ficha de planeta </th>

                <th> Nombre</th>

                <th> Tipo</th>

                <th> Numero de anillos</th>


            </tr>

            @for (int i = 0; i < 8; i++)
            {
                <tr>
                    <td> @ViewBag.planeta[i].id</td>
                    <td>@ViewBag.planeta[i].nombre</td>

                    <td> @ViewBag.planeta[i].tipo</td>

                    <td> @ViewBag.planeta[i].numeroAnillos</td>

                </tr>
            }
    </div>




</body>
</html>

